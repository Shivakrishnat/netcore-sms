{
    "name": "Shivakrishnat/netcore-sms",
    "description": "NetCore SMS Notification",
    "homepage": "https://github.com/Shivakrishnat/netcore-sms",
    "minimum-stability": "dev",
    "license": "MIT",
    "require": {
        "php": ">=5.6.4",
        "illuminate/notifications": "^5.3",
        "illuminate/support": "^5.1|^5.2|^5.3",
        "illuminate/queue": "^5.3",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\NetCoreSMS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\NetCoreSMS\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
